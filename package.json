{
  "name": "Paradise-Nursery-Shopping-Application",
  "private": true,
  "homepage":"https://wexeljose.github.io/Paradise-Nursery-Shopping-Application/",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite build; vite preview --host",
    "gh-deploy": "git push origin :gh-pages && git subtree push --prefix dist origin gh-pages",
    "gh-deploy-init": "git push origin && git subtree push --prefix dist origin gh-pages"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "gh-pages": "^6.2.0",
    "vite": "^5.2.0"
  }
}
